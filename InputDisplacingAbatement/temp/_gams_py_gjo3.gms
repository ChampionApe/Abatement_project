$offlisting
p.lo[n]$((a_FG[n])) = -inf;
p.up[n]$((a_FG[n])) = inf;
q.lo[n]$((sector_FG[n] and not output_FG[n])) = -inf;
q.up[n]$((sector_FG[n] and not output_FG[n])) = inf;
$onlisting


# ----------------------------------------------------------------------------------------------------
#  Define FG model
